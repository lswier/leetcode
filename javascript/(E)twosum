/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    
    // O(nlog(n)) time
    // Does not complete full repetition on inner loop
    // Because we have already calculated that combination.
    
    for (let i = 0; i < nums.length; i++){
        for (let j = i+1; j < nums.length; j++){
            if (nums[i] + nums[j] == target){
                return [i,j];
            }
        }       
    }
};
